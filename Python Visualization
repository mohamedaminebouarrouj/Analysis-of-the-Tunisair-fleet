  {
   "cell_type": "code",
   "execution_count": 514,
   "metadata": {},
   "outputs": [],
   "source": [
    "Air_Algerie=airlines.loc[airlines['Airline'] == \"Air Algerie\"]\n",
    "Egypt_Air=airlines.loc[airlines['Airline'] == \"EgyptAir\"]\n",
    "Ethiopian_Airlines=airlines.loc[airlines['Airline'] == \"Ethiopian Airlines\"]\n",
    "RA_Maroc=airlines.loc[airlines['Airline'] == \"Royal Air Maroc\"]\n",
    "Trans_avia=airlines.loc[airlines['Airline'] == \"Transavia\"]\n",
    "Turkish_Airlines=airlines.loc[airlines['Airline'] == \"Turkish Airlines\"]\n",
    "Tunis_Air=airlines.loc[airlines['Airline'] == \"Tunisair\"]\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 515,
   "metadata": {},
   "outputs": [],
   "source": [
    "import warnings\n",
    "def changeFormat(airline):\n",
    "    warnings.filterwarnings('ignore')\n",
    "    airline=airline.reset_index(drop=True)\n",
    "    for i in range(len(airline)):\n",
    "        airline[\"Total Cost (Current)\"][i]=airline[\"Total Cost (Current)\"][i].replace(\",\",\"\")\n",
    "        \n",
    "    for i in range(len(airline)):\n",
    "        airline[\"Total Cost (Current)\"][i]=airline[\"Total Cost (Current)\"][i][1:50]\n",
    "        airline['Total Cost (Current)'][i]=float(airline['Total Cost (Current)'][i])\n",
    "    return airline"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 516,
   "metadata": {},
   "outputs": [],
   "source": [
    "Air_Algerie=changeFormat(Air_Algerie)\n",
    "Egypt_Air=changeFormat(Egypt_Air)\n",
    "Ethiopian_Airlines = changeFormat(Ethiopian_Airlines)\n",
    "RA_Maroc=changeFormat(RA_Maroc)\n",
    "Turkish_Airlines=changeFormat(Turkish_Airlines)\n",
    "Tunis_Air=changeFormat(Tunis_Air)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 586,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "\n",
