    "Algerie=PIB.loc[PIB['Country Code']=='DZA']\n",
    "Egypte=PIB.loc[PIB['Country Code']=='EGY']\n",
    "Maroc=PIB.loc[PIB['Country Code']=='MAR']\n",
    "\n"
   ]
